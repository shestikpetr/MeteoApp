# Example local.properties file
# Copy this file to local.properties and set your actual API keys and configuration

# Yandex MapKit API Key
# Get your API key from: https://developer.tech.yandex.ru/services
YANDEX_MAPKIT_API_KEY=YOUR_YANDEX_MAPKIT_API_KEY

# API Configuration
# Base URL for the Meteo API (include protocol and full path)
DEFAULT_BASE_URL=https://your-api-server.com/api/v1/

# API Host (hostname or IP address for certificate pinning)
# This should match the hostname/IP in DEFAULT_BASE_URL
API_HOST=your-api-server.com

# Android SDK location (usually auto-generated by Android Studio)
# sdk.dir=C\:\\Users\\YourUsername\\AppData\\Local\\Android\\Sdk